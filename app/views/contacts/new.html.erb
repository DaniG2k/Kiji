<% title 'Contact' %>
<div class="row">
  <div class="large-6 columns">
  	<%= form_for :contact do |f| %>
		  <p>
		    <%= f.label :email %><br>
		    <%= f.text_field :email %>
		  </p>
		  <p>
		    <%= f.label :text %><br>
		    <%= f.text_area :text %>
		  </p>
		  <p>
		  	<%= f.submit %>
			</p>
		<% end %>
  </div>
  <div class="large-6 columns">
  </div>
</div>
